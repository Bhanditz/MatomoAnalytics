{
	"name": "MatomoAnalytics",
	"author": "John Lewis",
	"url": "https://github.com/miraheze/MatomoAnalytics",
	"descriptionmsg": "matomoanalytics-desc",
	"type": "specialpage",
	"AvailableRights": [
		"noanalytics"
	],
	"MessagesDirs": {
		"MatomoAnalytics": [
			"i18n"
		]
	},
	"AutoloadClasses": {
		"MatomoAnalytics": "includes/MatomoAnalytics.php",
		"MatomoAnalyticsHooks": "includes/MatomoAnalyticsHooks.php"
	},
	"Hooks": {
		"LoadExtensionSchemaUpdates": [
			"MatomoAnalyticsHookss::matomoAnalyticsSchemaUpdates"
		],
		"SkinAfterBottomScripts": [
			"MatomoAnalyticsHooks::matomoScript"
		]
	},
	"config": {
		"MatomoAnaltyicsDatabase": false,
		"MatomoAnalyticsServerURL": false,
		"MatomoAnalyticsSiteID": 1,
		"MatomoAnalyticsTokenAuth": false,
		"MatomoAnalyticsUseDB": false
	},
	"manifest_version": 1
}
